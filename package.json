{
  "name": "parcel-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate using Parcel and Sass",
  "scripts": {
    "build": "parcel build public/index.html --out-dir dist --public-url ./",
    "dev": "npm run clean && parcel public/index.html --out-dir development -p 3000",
    "clean": "rm -rf ./development && rm -rf ./.cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/notSalt/parcel-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "parcel",
    "sass"
  ],
  "author": "_notSalt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/notSalt/parcel-boilerplate/issues"
  },
  "homepage": "https://github.com/notSalt/parcel-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/runtime-corejs2": "^7.9.2",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.3"
  }
}
